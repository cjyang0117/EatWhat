<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".questionSuggestAct2"
    tools:layout_editor_absoluteY="81dp">

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:onClick="gotoSearchAct"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button67"
        app:layout_constraintTop_toTopOf="@+id/button67"
        app:srcCompat="@android:drawable/ic_menu_search" />

    <Button
        android:id="@+id/button64"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:onClick="gotoRandomSuggestAct"
        android:text="@string/randomSuggest"
        app:layout_constraintBaseline_toBaselineOf="@+id/button65"
        app:layout_constraintEnd_toStartOf="@+id/button65"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button64"
        android:onClick="gotoQuestionSuggestAct"
        android:text="@string/questionSuggest"
        app:layout_constraintEnd_toStartOf="@+id/button67"
        app:layout_constraintStart_toEndOf="@+id/button64"
        app:layout_constraintTop_toBottomOf="@+id/button54" />

    <Button
        android:id="@+id/button66"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button65"
        android:onClick="gotoMain2Activity"
        android:text="@string/mainAct"
        app:layout_constraintBaseline_toBaselineOf="@+id/button65"
        app:layout_constraintStart_toEndOf="@+id/button65" />

    <Button
        android:id="@+id/button67"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button66"
        android:onClick="gotoRecordAct"
        android:text="@string/record"
        app:layout_constraintBaseline_toBaselineOf="@+id/button66"
        app:layout_constraintEnd_toStartOf="@+id/imageButton13"
        app:layout_constraintStart_toEndOf="@+id/button65" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="148dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/button54"
        android:onClick="ILike"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/button55"
        android:layout_marginStart="31dp"
        android:text="OK"
        app:layout_constraintBaseline_toBaselineOf="@+id/button55"
        app:layout_constraintEnd_toStartOf="@+id/button55"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button55"
        android:onClick="Soso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="174dp"
        android:text="都行"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button56"
        app:layout_constraintStart_toEndOf="@+id/button54" />

    <Button
        android:id="@+id/button56"
        android:onClick="Notlike"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button55"
        android:layout_marginEnd="36dp"
        android:text="no"
        app:layout_constraintBaseline_toBaselineOf="@+id/button55"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button55" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="260dp"
        android:id="@+id/question"
        />
</RelativeLayout>